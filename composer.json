{
    "name": "newelement/shoppe",
    "description": "A Laravel CMS eCommerce package",
    "keywords": ["laravel", "cms", "admin", "shop", "ecommerce"],
    "license": "MIT",
    "homepage": "https://newelementdesigns.com/neutrino/",
    "support": {
        "issues": "https://github.com/newelement/shoppe/issues",
        "source": "https://github.com/newelement/shoppe"
    },
    "authors": [
        {
            "name": "Don Jones",
            "email": "newelementdesigns@me.com"
        }
    ],
    "require": {
        "newelement/neutrino": "1.*",
        "shippo/shippo-php": "1.*",
        "avalara/avataxclient": "*",
        "taxjar/taxjar-php": "^1.8",
        "stripe/stripe-php": "^7.23"
    },
    "autoload": {
        "psr-4": {
            "Newelement\\Shoppe\\": "src/"
        }
    },
	"autoload-dev": {
        "psr-4": {
            "Newelement\\Shoppe\\Tests\\": "tests"
    	}
	},
	"minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Newelement\\Shoppe\\ShoppeServiceProvider"
            ],
			"aliases": {
            	"Shoppe": "Newelement\\Shoppe\\Facades"
        	}
        }
    }
}
